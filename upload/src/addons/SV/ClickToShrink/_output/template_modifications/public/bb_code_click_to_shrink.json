{
    "template": "bb_code.less",
    "description": "Click to shrink LESS",
    "execution_order": 10,
    "enabled": true,
    "action": "str_replace",
    "find": ".bbCodeBlock-expandLink\n{",
    "replace": ".bbCodeBlock--expandable.is-expanded > div > .bbCodeBlock-shrinkLink\n{\n\tdisplay:block;\n\theight:20px;\n\tposition:static;\n}\n\n.bbCodeBlock-shrinkLink,\n$0"
}